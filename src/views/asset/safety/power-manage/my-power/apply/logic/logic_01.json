    "onFormCreated": "",
    "onFormMounted": "const baseurl = sessionStorage.getItem('baseurl')\nlet businessSegmentR = this.getWidgetRef('businessSegment1')\naxios.post(`${baseurl}/planBusiness/listBusiness`).then(function(res){\n  if(res.data.flag) {\n    const data = res.data.data.map(ele => {\n      return {label:ele.busin_name, value:ele.busin_id}\n    })\n    businessSegmentR.loadOptions(data)\n  }\n})\n\nlet logicTableR = this.getWidgetRef('logicTable1')\naxios.post(`${baseurl}/authority/getTables`,{type:0}).then(function(res){\n  if(res.data.flag) {\n    const data = res.data.data.map(ele => {\n      return {label:ele.cube_code, value:ele.cube_id}\n    })\n    localStorage.setItem('logicList',JSON.stringify(data))\n    logicTableR.loadOptions(data)\n  }\n})",
