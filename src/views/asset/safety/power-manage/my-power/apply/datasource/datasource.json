    "onFormCreated": "",
    "onFormMounted": "const baseurl = sessionStorage.getItem('baseurl')\nlet datasourceR = this.getWidgetRef('datasource')\nconst data = {pageSize:null,pageNo:null,data_source_name:null,data_souce_type:null}\naxios.post(`${baseurl}/planDatasource/list`,data).then(function(res){\n  if(res.data.flag) {\n    const data = res.data.data.map(ele => {\n      return {label:ele.data_source_name, value:ele.id}\n    })\n    datasourceR.loadOptions(data)\n  }\n})\n\n\nlet projectR = this.getWidgetRef('project')\naxios.post(`${baseurl}/planProject/listPage`).then(function(res){\n  if(res.data.flag) {\n    const data = res.data.data.map(ele => {\n      return {label:ele.project_name, value:ele.id}\n    })\n    projectR.loadOptions(data)\n  }\n})\n\n\n\n",
